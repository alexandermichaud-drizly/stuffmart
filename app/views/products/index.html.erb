<h1>Welcome to Stuffmart</h1>
<h2>Browse our stuff</h2>

<ul>
  <% @products.each do |product| %>
    <li>
      <div><%= product.name %></div>
      <div><%= product.description %></div>
      <div><%= product.price %></div>
      <div><%= product.stock %></div>
      <div><%= product.category %></div>
    </li>
  <% end %>
</ul>
